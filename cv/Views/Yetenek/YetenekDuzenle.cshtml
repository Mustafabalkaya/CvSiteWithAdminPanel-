@model cv.Models.Entity.TblYeteneklerim

@{
    ViewBag.Title = "YetenekDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Yetenek Düzenleme Sayfası</h2>
<br />
<form method="post">
    @Html.Label("Yetenek Açıklaması")
    @Html.TextBoxFor(x => x.YETENEK, new { @class = "form-control" })<br />
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.ORAN, new { @class = "form-control" })<br />
    <button class="btn btn-success">Güncelle</button>
</form>